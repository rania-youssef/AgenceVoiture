<script>

  async function start() {
    const response = await fetch("https://vpic.nhtsa.dot.gov/api/vehicles/getmodelsformake/honda?format=json");
    const data = await response.json();
    console.log(data.Results);
    var modelList = data.Results;

    var text = `<select name="voiture" onchange='this.form.submit()'> <option > Choose a model </option>`;

    for (x in modelList) {

      text += `<option value="${modelList[x].Model_Name}"> ${modelList[x].Model_Name}</option>`;
    }
    text += `</select> `;

    document.getElementById("sell").innerHTML = text;

  }

  start();
</script>
